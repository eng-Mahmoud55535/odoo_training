<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="category_crm_lead" model="ir.module.category">
        <field name="name">CRM Lead Category</field>
        <field name="description">CRM Lead Customizations Category</field>
        <field name="sequence">100</field>
    </record>

    <record id="group_crm_lead_user" model="res.groups">
        <field name="name">User</field>
        <field name="category_id" ref="category_crm_lead"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_admin'))]"/>
    </record>

    <record id="group_crm_lead_manager" model="res.groups">
        <field name="name">Manager</field>
        <field name="category_id" ref="category_crm_lead"/>
        <field name="implied_ids" eval="[(4, ref('base.group_erp_manager'))]"/>
        <field name="users" eval="[(4, ref('base.user_admin'))]"/>
    </record>

    <record id="group_crm_lead" model="res.groups">
        <field name="name">CRM Lead Group</field>
        <field name="category_id" ref="base.module_category_sales_crm"/>
        <field name="implied_ids" eval="[(4, ref('group_crm_lead_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_admin'))]"/>
    </record>
</odoo>